<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Web UI Test Suite"  thread-count="3" verbose="1" parallel="classes" data-provider-thread-count="2">
    <listeners>
        <listener class-name="com.starttech.listener.ExecutionListener"/>
        <listener class-name="com.starttech.listener.TestListener"/>
        <listener class-name="com.starttech.listener.AnnotationTransformer"/>
    </listeners>
    <test name="Run Failed Tests">
        <classes>
            <class name="skeleton.RunnerFailed"/>
        </classes>
    </test>
</suite>

