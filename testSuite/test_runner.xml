<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Web UI Test Suite" parallel="tests" data-provider-thread-count="1">
    <listeners>
        <listener class-name="com.starttech.listener.ExecutionListener"/>
        <listener class-name="com.starttech.listener.TestListener"/>
        <listener class-name="com.starttech.listener.AnnotationTransformer"/>
    </listeners>
    <test name="Run All Tests">
        <classes>
            <class name="skeleton.RunCucumberTest"/>
        </classes>
    </test>
</suite>